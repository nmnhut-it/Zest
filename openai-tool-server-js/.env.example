# Server configuration
PORT=8000
NODE_ENV=development

# Zest Agent Proxy URL (optional - will auto-discover if not set)
# ZEST_PROXY_URL=http://localhost:8765

# API Key for authentication (optional - no auth if not set)
# OPENAI_TOOL_SERVER_API_KEY=your-secret-api-key-here

# Logging level
LOG_LEVEL=info

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
